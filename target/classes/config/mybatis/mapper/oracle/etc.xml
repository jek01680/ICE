<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.mybatis.mapper.oracle.etc">
	<select id="DeptAll" resultType="departmentVO">
		select * from department
		where deptcode != 999
		order by deptcode
	</select>
	
	<select id="PosAll" resultType="positionVO">
		select * from position
		where poscode != 999
		order by poscode
	</select>
		
</mapper>









