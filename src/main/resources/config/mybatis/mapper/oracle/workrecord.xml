<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.mybatis.mapper.oracle.workrecord">
	<insert id="insertWork" parameterType="WorkRecordVO">
		<selectKey keyProperty="cmtNo" resultType="int" order="BEFORE">
			select workRecord_seq.nextval from dual
		</selectKey>
		insert into workRecord
		values(cmtNo,#{memNo},#{cmpIn},#{cmpOut},#{cmpStatus})
	</insert>
	
	<select id="selectWork" parameterType="string" resultType="WorkRecordVO">
		select * from workRecord where cmtNo = #{cmtNo}
	</select>
	
</mapper>









